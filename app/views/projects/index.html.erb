<p id="notice"><%= notice %></p>

<h1>Projects</h1>

<table>
  <thead>
    <tr>
      <th>title</th>
      <th>Summaryofchallenge</th>
      <th>Summaryofsolution</th>
      <th>Summaryofoutcome</th>
      <th>Challenge</th>
      <th>Solution</th>
      <th>Outcome</th>
      <th>Comment</th>
      <th>Order</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.title %></td>
        <td><%= project.summaryOfChallenge %></td>
        <td><%= project.summaryOfSolution %></td>
        <td><%= project.summaryOfOutcome %></td>
        <td><%= project.challenge %></td>
        <td><%= project.solution %></td>
        <td><%= project.outcome %></td>
        <td><%= project.comment %></td>
        <td><%= project.order %></td>
        <td><%= project.category %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path %>
